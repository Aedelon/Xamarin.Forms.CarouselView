<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="12.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <!--declare well known targets-->
  <PropertyGroup>
    <BuildDependsOn />
    <CleanDependsOn />
    <InstallDependsOn />
    <CompileDependsOn />
    <GetTargetPathDependsOn />
    <NugetRestoreDependsOn />
    <PackageDependsOn />
    <SignDependsOn />
  </PropertyGroup>
  
  <Target Name="Build" Outputs="@(TargetResult)" DependsOnTargets="$(BuildDependsOn)" />
  <Target Name="Clean" DependsOnTargets="$(CleanDependsOn)" />
  <Target Name="Install" DependsOnTargets="$(InstallDependsOn)" />
  
  <!--VS intellisense calls Compile which calls csc which provides the resolve assemblies and any build errors-->
  <!--see also: https://msdn.microsoft.com/en-us/library/ms171468.aspx?f=255&MSPPError=-2147217396-->
  <Target Name="Compile" Outputs="@(TargetResult)" DependsOnTargets="$(CompileDependsOn)" />
  <Target Name="GetTargetPath" Outputs="@(TargetResult)" DependsOnTargets="$(GetTargetPathDependsOn)" />
  
  <!--should be a standard VS target-->
  <Target Name="NugetRestore" DependsOnTargets="$(NugetRestoreDependsOn)" />
  
  <!--android-->
  <Target Name="Package" DependsOnTargets="$(PackageDependsOn)" />
  <Target Name="Sign" DependsOnTargets="$(SignDependsOn)" />

</Project>