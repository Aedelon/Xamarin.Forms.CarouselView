<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="14.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <!--

    Properties
  -->
  <PropertyGroup>
    <MetaProjectGuid>{96CAA386-638A-4131-9DCB-66C61447BAFA}</MetaProjectGuid>
  </PropertyGroup>
  <Import Project="$([MSBuild]::GetDirectoryNameOfFileAbove($(MSBuildThisFileDirectory), .pre.props))\.pre.props" />
  <Import Project="$([MSBuild]::GetDirectoryNameOfFileAbove($(MSBuildThisFileDirectory), .props))\.props" />
  <!--
  
    Hierarchy
  -->
  <ItemGroup>
    <!--all E { mobile }-->
    <PlatformGroup Include="$(AllPlatformId)">
      <Platforms>$(MobilePlatformId)</Platforms>
    </PlatformGroup>
    <!--mobile E { android, ios }-->
    <PlatformGroup Include="$(MobilePlatformId)">
      <Platforms>$(AndroidMobilePlatformId);$(IosMobilePlatformId)</Platforms>
    </PlatformGroup>

    <!--android E { android.aut }-->
    <PlatformGroup Include="$(AndroidMobilePlatformId)">
      <Platforms>$(AndroidAutomationLibraryPlatformId)</Platforms>
    </PlatformGroup>
    <!--android.aut E { android.aut-4.5 }-->
    <PlatformGroup Include="$(AndroidAutomationLibraryPlatformId)">
      <Platforms>$(AndroidAutomationLibraryPlatformId-4_5)</Platforms>
    </PlatformGroup>

    <!--ios E { ios.aut }-->
    <PlatformGroup Include="$(IosMobilePlatformId)">
      <Platforms>$(IosAutomationLibraryPlatformId)</Platforms>
    </PlatformGroup>
    <!--ios.aut E { ios.aut-4.5 }-->
    <PlatformGroup Include="$(IosAutomationLibraryPlatformId)">
      <Platforms>$(IosAutomationLibraryPlatformId-4_5)</Platforms>
    </PlatformGroup>
  </ItemGroup>
  <!--

    Universal
  -->
  <PropertyGroup Condition=" '$(Platform)' == '[none]' "/>
  <PropertyGroup>
    <OutputType>Library</OutputType>
    <ProjectGuid>{96F54817-DA0A-4F40-A69C-0040BF5BE95D}</ProjectGuid>
    <AppDesignerFolder>Properties</AppDesignerFolder>
    <RootNamespace>CarouselGallery.UITest</RootNamespace>
    <AssemblyName>CarouselGallery.UITest</AssemblyName>
  </PropertyGroup>
  <!--

    Library
  -->
  <ItemGroup Condition="'$(IsLibraryPlatform)'=='true'">
    <Compile Include="$(SrcDir)\GlobalAssemblyInfo.cs">
      <Link>Properties\GlobalAssemblyInfo.cs</Link>
      <IsMxbuildTemplate>true</IsMxbuildTemplate>
    </Compile>
    <Compile Include="$(ExtIdDir)BuildInfo.cs">
      <DependentUpon>BuildInfo.t.cs</DependentUpon>
      <Link>Properties\BuildInfo.cs</Link>
    </Compile>
    <None Include="$(ExtIdDir)BuildInfo.t.cs">
      <IsMxbuildTemplate>true</IsMxbuildTemplate>
      <Link>Properties\BuildInfo.t.cs</Link>
      <CompileOutput>BuildInfo.cs</CompileOutput>
    </None>
    <Compile Include="$(ExtPackDir)PackageInfo.cs">
      <Link>Properties\PackageInfo.cs</Link>
    </Compile>
  </ItemGroup>
  <!--

    Shared
  -->
  <ImportGroup>
    <Import Project="$(XamarinUITestShim)" />
    <Import Project="$(NugetNunitShim)" />
  </ImportGroup>
  <ItemGroup>
    <!--references-->
    <Reference Include="System" />
    <Reference Include="System.Core" />
    <Reference Include="System.Xml.Linq" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Data" />
    <Reference Include="System.Net.Http" />
    <Reference Include="System.Xml" />
    <!--nuget packages-->
    <NugetPackage Include="NUnit.Runners">
      <Version>$(NugetNunitVersion)</Version>
    </NugetPackage>
    <!--source-->
    <Compile Include="AppSetup.cs" />
    <Compile Include="CarouselViewUITest.cs" />
  </ItemGroup>
  <!--

    Android
  -->
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'debug|android.aut' " />
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'release|android.aut' " />
  <!--

    iOS
  -->
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'debug|ios.aut' " />
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'release|ios.aut' " />
  <!--

    Targets
  -->
  <Import Project="$([MSBuild]::GetDirectoryNameOfFileAbove($(MSBuildThisFileDirectory), .targets))\.targets" />
</Project>