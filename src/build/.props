<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="12.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Project="$([MSBuild]::GetDirectoryNameOfFileAbove($(MSBuildThisFileDirectory).., .props))\.props" />

  <PropertyGroup>
    <TargetFrameworkVersion>v4.5.2</TargetFrameworkVersion>
  </PropertyGroup>

  <!--shim-->
  <PropertyGroup>
    <ShimTarget>NugetRestore</ShimTarget>
    <ShimInclude>$(MSBuildThisFileDirectory)**\*.csproj</ShimInclude>
    <ShimExclude/>
  </PropertyGroup>

  <!--meta-->
  <PropertyGroup>
    <BuildName>Setup</BuildName>
  </PropertyGroup>
</Project>