<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="12.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <PropertyGroup>
    <NugetXamarinUITestVersion>1.3.7</NugetXamarinUITestVersion>
    <NugetXamarinUITestTargetFramework>net452</NugetXamarinUITestTargetFramework>
    <NugetXamarinUITestLibDir>$(NugetPackagesDir)Xamarin.UITest.$(NugetXamarinUITestVersion)\lib\</NugetXamarinUITestLibDir>
  </PropertyGroup>

  <ItemGroup>
    <NugetReference Include="Xamarin.UITest, Version=$(NugetXamarinUITestVersion).0, Culture=neutral, processorArchitecture=MSIL">
      <Package>Xamarin.UITest</Package>
      <Version>$(NugetXamarinUITestVersion)</Version>
      <TargetFramework>$(NugetXamarinUITestTargetFramework)</TargetFramework>
      <HintPath>$(NugetXamarinUITestLibDir)Xamarin.UITest.dll</HintPath>
      <Private>True</Private>
    </NugetReference>
  </ItemGroup>
      
</Project>