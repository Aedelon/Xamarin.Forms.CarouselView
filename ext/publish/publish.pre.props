<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="12.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <PropertyGroup>
    <ExtPublishDir>$(MSBuildThisFileDirectory)</ExtPublishDir>
    <PublishProj>$(ExtPublishDir)publish.proj</PublishProj>
  </PropertyGroup>
  
  <!--build drop paths-->
  <PropertyGroup>
    <DropDir>$(RootDir)drp\</DropDir>
    <BuildByNumberDir>$(DropDir)number\</BuildByNumberDir>
    <BuildByRevisionDir>$(DropDir)revision\</BuildByRevisionDir>
    <BuildByVersionDir>$(DropDir)version\</BuildByVersionDir>
  </PropertyGroup>

  <!--results (build and enlistment info generated by shim.proj)-->
  <ItemGroup>
    <BuildInfoFile Include="$(BuildBinDir)BuildInfo.cs"/>
    <BuildNumberFile Include="$(BuildBinDir)BuildInfo.Build.number" />
    <EnlistmentStatusFile Include="$(BuildBinDir)BuildInfo.Enlistment.status" />
    <EnlistmentRevisionFile Include="$(BuildBinDir)BuildInfo.Enlistment.revision" />
    <EnlistmentBranchFile Include="$(BuildBinDir)BuildInfo.Enlistment.branch" />
    <EnlistmentUrlFile Include="$(BuildBinDir)BuildInfo.Enlistment.url" />
  </ItemGroup>
  <PropertyGroup>
    <InvalidEnlistmentRevision>dirty</InvalidEnlistmentRevision>
    <InvalidBuildNumber>0</InvalidBuildNumber>
  </PropertyGroup>

</Project>