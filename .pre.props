<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="12.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  
  <!--ext paths-->
  <PropertyGroup>
    <RootDir>$(MSBuildThisFileDirectory)</RootDir>
    <ExtDir>$(RootDir)ext\</ExtDir>
  </PropertyGroup>
  <Import Project="$(ExtDir)common.pre.props"/>

  <!--verbosity-->
  <PropertyGroup>
    <Verbosity>normal</Verbosity>
  </PropertyGroup>

  <!--ext paths-->
  <PropertyGroup>
    <ExtCiDir>$(ExtDir)ci\</ExtCiDir>
    <ExtNugetDir>$(ExtDir)nuget\</ExtNugetDir>
    <ExtShimDir>$(ExtDir)shim\</ExtShimDir>
    <ExtMetaDir>$(ExtDir)meta\</ExtMetaDir>
    <ExtXfDir>$(ExtDir)xf\</ExtXfDir>
  </PropertyGroup>

  <Import Project="$(ExtMetaDir)meta.pre.props"/>
</Project>